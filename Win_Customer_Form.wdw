   -
     code : |1-
      PROCEDURE Win_Customer_Form(sMode=<§$0000§>,nCustomerNum=0)
      
      SWITCH sMode
      	CASE <§$0000§> :
      		HReset(CUSTOMER)
      		FileToScreen(Win_Customer_Form,CUSTOMER)
      	CASE <§$0001§> :
      		// Read the record
      		HReadSeekFirst(CUSTOMER,<§$0002§>,nCustomerNum)
      		IF NOT HFound(CUSTOMER) THEN
      			Error(<§$0003§>)
      			Close
      		END
      		FileToScreen(Win_Customer_Form,CUSTOMER)
      	OTHER CASE
      		Error(StringBuild(<§$0004§>,sMode))
      		Close
      END
