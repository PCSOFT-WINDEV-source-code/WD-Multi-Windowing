        code : |1-
         SWITCH sMode
         	CASE <§$0000§> :
         		// Retrieve the information on the screen
         		ScreenToFile(Win_Customer_Form,CUSTOMER)
         		
         		// Add the customer form
         		HAdd(CUSTOMER)
         		IF ErrorOccurred THEN
         			Error(<§$0005§>,HErrorInfo())
         		END
         		nCustomerNum = CUSTOMER.CUSTOMERID
         		
         		// Update the table
         		IF WinStatus(Win_CustomerTable)<>NotFound  THEN
         			TableDisplay(Win_CustomerTable.Table_CUSTOMER,taCurrentFirst)
         		END		
         		Close
         	CASE <§$0001§> :
         		// Note: No need to reposition, window with an independent HF context
         				
         		// Retrieve the information on the screen
         		ScreenToFile(Win_Customer_Form,CUSTOMER)
         		
         		// Modify the customer form
         		HModify(CUSTOMER)
         		IF ErrorOccurred THEN
         			Error(<§$0006§>,HErrorInfo())
         		END	
         		
         		// Update the table
         		IF WinStatus(Win_CustomerTable)<>NotFound  THEN
         			TableDisplay(Win_CustomerTable.Table_CUSTOMER,taCurrentFirst)
         		END
         		Close	
         	OTHER CASE
         		Error(<§$0004§>+sMode)		
         		Close
         END
